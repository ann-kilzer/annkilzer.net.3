        "dotenv": "^16.3.1",
        "iconoir-react": "7.0.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.20.0"
    },
    "devDependencies": {
        "@playwright/test": "1.40.0",
        "@stylistic/eslint-plugin-ts": "1.4.1",
